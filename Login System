print("Vinta Login System")
print()
def login():
  attempts = 0
  while True:
    username = input("What is your username?: ")
    password = input("What is your password?: ")
    if username == "Vinta" and password == "Flowers":
      print("Welcome Vinta!")
      break
    else:
      print("That is not the correct username or password. Try again!")
      attempts +=1
      continue

login()
